{
	"ArtifactsDest": "downloads",
	"Tasks": [
		"interpolate-source",
		"go-vet",
		"xc",
		"archive-zip",
		"archive-tar-gz"
	],
	"TaskSettings": {
	"BuildConstraints": "linux,amd64 windows,amd64 darwin,amd64",
	"Resources": {
            "Include": "INSTALL*,README*,LICENSE*,config/*,static/*,templates/*"
	},
	"ConfigVersion": "0.9",
	"PackageVersion": "0.0.2",
	"Env": [
		"GOPATH=Godeps{{.PLS}}{{.Env.GOPATH}}"
	]
}
