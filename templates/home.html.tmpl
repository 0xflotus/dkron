{{define "content"}}
<div class="container">
  <div class="row">
  </div>

      <table class="table table-striped">
	<tr>
	  <th>Job</th>
	  <th>Last</th>
	</tr>
        {{ range $job := .Jobs }}
        <tr>
	  <td>{{ $job.Name }}</td>
	  <td>{{ $job.LastSuccess }}</td>
	  <td>{{ if isSuccess }}Success{{ end }}</td>
	</tr>
	{{ end }}
      </table>
</div>
{{end}}
