{{define "content"}}
<div class="container">
  <div class="row">
  </div>

      <table class="table table-striped">
	<tr>
	  <th>Job</th>
	  <th>Last</th>
	</tr>
        {{ range $job := .Jobs }}
        <tr>
	  <td><a href="/dashboard/jobs/{{$job.Name}}/executions">{{ $job.Name }}</a></td>
	  <td class="error">{{if isSuccess $job}}Success{{else}}Failed{{end}}</td>
	</tr>
	{{ end }}
      </table>
</div>
{{end}}
